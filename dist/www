ed;\n\n  instance.init(doc, opts, function(err) {\n    if (err) {\n      return cb(err);\n    }\n    cb(null, instance);\n  });\n}\n\nmodule.exports = QueryCursor;\n\n\n//# sourceURL=webpack:///./node_modules/mongoose/lib/cursor/QueryCursor.js?");

