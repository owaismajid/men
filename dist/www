eval("\n\nvar inherits = __webpack_require__(/*! util */ \"util\").inherits,\n  EventEmitter = __webpack_require__(/*! events */ \"events\").EventEmitter,\n  Connection = __webpack_require__(/*! ./connection */ \"./node_modules/mongodb-core/lib/connection/connection.js\"),\n  MongoError = __webpack_require__(/*! ../error */ \"./node_modules/mongodb-core/lib/error.js\").MongoError,\n  MongoNetworkError = __webpack_require__(/*! ../error */ \"./node_modules/mongodb-core/lib/error.js\").MongoNetworkError,\n  Logger = __webpack_require__(/*! ./logger */ \"./node_modules/mongodb-core/lib/connection/logger.js\"),\n  f = __webpack_require__(/*! util */ \"util\").format,\n  Query = __webpack_require__(/*! ./commands */ \"./node_modules/mongodb-core/lib/connection/commands.js\").Query,\n  CommandResult = __webpack_require__(/*! ./command_result */ \"./node_modules/mongodb-core/lib/connection/command_result.js\"),\n  MESSAGE_HEADER_SIZE = __webpack_require__(/*! ../wireprotocol/shared */ \"./node_modules/mongodb-core/lib/wireprotocol/shared.js\").MESSAGE_HEADER_SIZE,\n  opcodes = __webpack_require__(/*! ../wireprotocol/shared */ \"./node_modules/mongodb-core/lib/wireprotocol/shared.js\").opcodes,\n  compress = __webpack_require__(/*! ../wireprotocol/compression */ \"./node_modules/mongodb-core/lib/wireprotocol/compression.js\").compress,\n  compressorIDs = __webpack_require__(/*! ../wireprotocol/compression */ \"./node_modules/mongodb-core/lib/wireprotocol/compression.js\").compressorIDs,\n  uncompressibleCommands = __webpack_require__(/*! ../wireprotocol/compression */ \"./node_modules/mongodb-core/lib/wireprotocol/compression.js\").uncompressibleCommands,\n  resolveClusterTime = __webpack_require__(/*! ../topologies/shared */ \"./node_modules/mongodb-core/lib/topologies/shared.js\").resolveClusterTime;\n\nconst apm = __webpack_require__(/*! ./apm */ \"./node_modules/mongodb-core/lib/connection/apm.js\");\n\nvar MongoCR = __webpack_require__(/*! ../auth/mongocr */ \"./node_m